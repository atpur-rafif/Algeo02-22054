{
  "type": "commonjs",
  "dependencies": {
    "express": "^4.18.2",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "build-app": "make build",
    "build-web": "node ./build.mjs",
    "dev-web": "node ./build.mjs -w",
    "dev-app": "nodemon -q --exec \"make build && echo\" -e cpp",
    "dev-all": "concurrently \"npm run dev-app\" \"npm run dev-web\" -n app,web",
    "build": "npm run build-app; npm run build-web",
    "all": "npm run build; node ./dist/main.js"
  },
  "devDependencies": {
    "@types/connect-livereload": "^0.6.2",
    "@types/express": "^4.17.20",
    "@types/livereload": "^0.9.4",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "connect-livereload": "^0.6.1",
    "cpy": "^10.1.0",
    "esbuild": "^0.19.5",
    "livereload": "^0.9.3",
    "nodemon": "^3.0.1",
    "react": "^18.2.0"
  }
}
